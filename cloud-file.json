{
  "services": [
    {
      "name": "app",
      "image": "",
      "target_num_containers": 1,
      "sequential_deployment": true,
      "autorestart": "ALWAYS",
      "autoredeploy": true,
      "linked_to_service": [
        {
          "name": "db",
          "to_service": "db"
        }
      ],
      "container_ports": [
        {
          "protocol": "tcp",
          "inner_port": 3000,
          "published": true,
          "outer_port": 3148
        }
      ],
      "container_envvars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        }
      ]
    },
    {
      "name": "db",
      "image": "mongo"
    }
  ],
  "name": "burgwyngo-bot-chat"
}